/* pages/list/list.wxss */
.pointImage{
   width:6vh;
   height:6vh;
   float: left
  
}
.point-item{
    font-size:2vh;
    text-overflow:ellipsis;
    width:30vh;
    display:block;
    white-space:nowrap;
    overflow:hidden;
    font-family: times
}
.point-title{
   font-size:2.5vh;
   margin-top:0.5vh;
   display: block;
   font-family: courier
}
.buttonClass{
 text-align: left;
 padding:2vh;
}
.point-distance{
    font-size: 2.5vh;
    font-family: fantasy;
    color: #82B302;
    display:block;
    float:right;
    margin:-6vh 0vh;
}
.title-image{
    width:3vh;
    height:3vh;
    float:left;
    margin-top:1vh;
}
.noData{
  width: 20vh;
  height: 20vh;
  z-index: 100;
  position:fixed;
  margin-left:37%;
  margin-top:50%;
}
.modal{
  position: fixed;
  z-index: 1000;
  width: 100%;
  height: 50%;
  
}
.commodity_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.3;
  overflow: hidden;
  z-index: 1000;
  color: #fff;
}
.commodity_attr_box {
  height: 337rpx;
  width: 100%;
  overflow: hidden;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 2000;
  background: #fff;
  padding-top: 0rpx;
}
.modalImage{
  width:5vh;
  height:5vh;
  margin-top:2vh;
  float:left;
  margin-left:15%;
  margin-right:1%;
}
.modalText{
  font-size:2vh;
  display:block;
  float:left;
  position:relative;
  white-space:nowrap;
  overflow:hidden;
  width: 90%;
  margin-left: 5%;
}
.modalTitle{
  display:block;
  margin-top:1vh;
  position:relative;
  float:left;
  margin-left:5%; 
}